#, kde-format
msgid "1 problem"
msgid_plural "%1 problems"
msgstr[0] "%1 បញ្ហា"

#: k3bsystemproblemdialog.cpp:100
msgid "Do not show again"
msgstr "កុំបង្ហាញ​ម្ដង​ទៀត​"

#: k3bsystemproblemdialog.cpp:105 option/k3bdevicewidget.cpp:57
